from abc import ABC, abstractmethod
import math
class Vehiculo(ABC):
    @abstractmethod
    def mover(self):
        pass
    @abstractmethod
    def detener(self):
        pass
class Coche(Vehiculo):
    def mover(self):
        print("El coche se estÃ¡ moviendo por la carretera ðŸš—")
    def detener(self):
        print("El coche se ha detenido ðŸ›‘")
class Bicicleta(Vehiculo):
    def mover(self):
        print("La bicicleta avanza pedaleando ðŸš²")
    def detener(self):
        print("La bicicleta se ha detenido con los frenos ðŸ›‘")
if __name__ == "__main__":
    coche = Coche()
    bici = Bicicleta()
    coche.mover()
    coche.detener()
    print("---------------")
    bici.mover()
    bici.detener()
